	body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,form,input,textarea,p{margin:0;padding:0;}
	ol,ul,li{list-style-type:none;}
	img{border:0 none;}
	h1,h2,h3,h4,h5,h6{font-size:12px;font-weight:normal;}
	b,em{font-style:normal;font-weight:normal;}
	a{color:#333;text-decoration:none;}
	body{padding:0 0 140px!important;_padding:0!important;position:relative;background:#efebe1;color:#333;font:12px/1.5 'Tahoma';min-width:1020px;}
	.clear{clear:both;}
	.red{color:#f00;}
	dt.shopping-main-tit i,.checked,.alt-tip b,.alt-tip i,.recommend .btn-d i,.recommend .btn-l,.recommend .btn-r,.recommend dl .push,.goconfirm div a,.shop .get .p-sale b,.shop .p-info a span .date,.n-right,.n-left,.tip,.Chead-app,.shop .p-info .more{background:url(../images/icon1.png?12) no-repeat;}
	.checkbox{position:relative;top:3px;display:inline-block;width:13px;height:13px; color:#f5f1f1;border:1px solid #ccc;background-color:#f5f1f1;cursor:pointer;}
	.checked{background-position:-15px -229px;border:none;padding:1px;}
.colspan6 a i,.Chead-info i{display:inline-block;width:0;height:0;font-size:0;overflow:hidden;}

.Chead{height:120px;background:url(../images/iconx.png) 0 -25px repeat-x;margin:0 0 30px;}
.Chead-main{position:relative;width:960px;height:120px;margin:0 auto;color:#999;z-index:90;}
.Chead-logo,.Chead-logo a{position:absolute;left:0;width:220px;height:110px;}
.Chead-logo{background:url(../images/new_logo.png) no-repeat;_filter:progid:DXImageTransform.Microsoft.AlphaImageLoader(src='../images/new_logo.png');_background-image:none;}
.Chead-main a{color:#999;} 
.Chead-info{position:absolute;right:0;z-index:101;width:1000px;height:15px;padding:5px 0;line-height:15px;}
.Chead-save,.Chead-welcome,.Chead-info .turl{position:relative;float:right;margin:0 20px 0 0;cursor:pointer;}
.Chead-welcome a,.Chead-welcome b{color:#c29721;}
.Chead-info .fast-nav,#Chead_myhome_btn{padding:0 11px 0 0;}
.Chead-myhome{display:none;position:absolute;top:25px;right:290px;width:60px;padding:5px 15px;background:#f1ebde;border-radius:5px;opacity:0.95;z-index:99;}
.Chead-myhome i{position:absolute;top:-10px;left:40px;z-index:99;border:5px dashed transparent;border-bottom:5px solid #ded8cd;}
.Chead-myhome a{color:#333;line-height:2;}
.Chead-myhome a:hover{color:#ca8519;}
.Chead-app{float:right;padding:0 0 0 21px;background-position:4px -298px;cursor:pointer;}
.Chead-info .turl i{position:absolute;top:6px;right:-2px;border:5px dashed transparent;border-top:5px solid #999;}
.Chead-floatmenu{display:none;position:absolute;right:100px;top:20px;width:155px;height:269px;padding:10px 8px;background:url(../images/fast_nav.png) no-repeat;filter:alpha(opacity=95);opacity:0.95;}
.Chead-floatmenu a{display:inline;float:left;width:48px;height:16px;overflow:hidden;margin:10px 14px 0;color:#333;}
.Chead-floatmenu a:hover{color:#ca8519;}

.Chead-appDown{display:none;position:absolute;right:-50px;top:25px;width:171px;height:249px;background:url(../images/appDown.png) no-repeat;}
.Chead-appDown a{position:absolute;left:35px;bottom:18px;width:112px;height:42px;background:url(about:blank);}


.Chead-main .jd1{position:absolute;top:48px;right:0;width:556px;height:30px;padding:35px 0 0 87px;line-height:30px;background:url(../images/bar1.png) no-repeat 216px top;}
.Chead-main .jd1 b{float:left;color:#666;padding:0 0 0 117px;white-space:nowrap;}
.Chead-main .jd1 .d{color:#c6a756;}



.Cfooter{position:absolute;_position:relative;_margin:50px 0 0;bottom:0;width:100%;background:#222;left:0px;}
.Cfooter-copyright{width:960px;padding:19px 0;margin:0 auto;color:#999;font:12px/18px Microsoft YaHei;}
.Cfooter-copyright a{color:#999;}

#float_btn{position:fixed;_position:absolute;bottom:20px;_top:expression(eval(documentElement.scrollTop+documentElement.clientHeight-this.clientHeight-20));left:50%;margin:0 0 0 510px;}
.customer_service,#scroll_Top{display:block;width:50px;height:50px;margin:0 0 10px;background:url(../images/common_float_ico1.png) no-repeat;cursor:pointer;}
.customer_service{background-position:0 -180px;display:none;}
.customer_service:hover{background-position:-60px -180px;}
#scroll_Top{visibility:hidden;margin:10px 0 0;}
#scroll_Top:hover{background-position:-60px 0;}

.turl:hover+div{
    display:block;
}
 .turl+div:hover{
    display:block;
}
