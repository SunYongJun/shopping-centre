@charset "utf-8";
/* CSS Document */
.sltBtnArea a,.sTabArea,.bindFloatH,.bindFloatH a,.bindFloat .bFconfirmBtn,.seeBtn,.openCartBtn,.bindingBtn,.sendPwdToMobile,.authenLi input,.paypwdHead,.authenBtn .submitBtn{background:url(http://img3.imglefeng.com/images/cart/cartico.gif) no-repeat;}
.sltCart{ width:770px;}
.sltAd img{ width:770px; height:145px; display:block;}
.sTabArea{width:768px;zoom:1;border:1px solid #ddd;margin:10px 0px 0px; clear:both;background-position:0px -222px; background-repeat:repeat-x; }
.tag{ zoom:1;width:770px; margin-top:-1px;}
.tag li	{ float:left; height:32px; line-height:32px; border-right:1px solid #ddd; border-top:1px solid #ddd; border-bottom:none; cursor:pointer; background-color:#f8f8f8;/* width:85px;*/ width:105px;}
.tag li.hui{ background-color:#b89987;color:#fff;}
.tag li a{ color:#666;/*width:85px;*/ width:105px;line-height:32px; font:bold 12/36px simsun; display:block;text-align:center;}
.tag li.on{ height:31px;background:#fff; position:relative; z-index:9; border-top:2px solid #000;}
.tag li.on a,.tag li.on a:hover{ line-height:28px; height:31px; font-weight:bold;top:1px; background:#fff; position:absolute;}
.tab_con{overflow:hidden;zoom:1;width:768px;border-top:1px solid #ddd; background:#fff;}
.tabarea{display:none;}
.sltBind{ padding:5px 0px 20px; overflow:hidden; zoom:1;}
.sltBind li{ margin-top:15px; clear:both; overflow:hidden; zoom:1;}
.sltBind li em{ display: inline; float:left; width:80px; height:26px; line-height:26px; padding:0px 5px; text-align:right; color:#666;}
.sltBind li input{ width:180px; padding:0px; padding-left:4px; height:24px; border:1px solid #ccc; /*line-height:32px;*/ line-height:24px; display:inline; float:left;}
.sltBind li label,.authenCon li label{ display:inline; float:left; margin-left:10px;}
.sltBind li input.yzmcode,.authenCon li input.yzmcode{width:90px;}
.sltBind li input.bthover{background: url(https://passport.lefeng.com/images/login_ico.png) no-repeat left -858px;}
.sltBind li span,.authenCon li span,.person_infor li span{ height:26px; line-height:26px; display:none; float:left; padding:0px 8px 0px 18px; margin-left:10px; background:url(https://passport.lefeng.com/images/login_ico.png) no-repeat left -546px; color:#6666;}
.sltBind li span.inner01,.authenCon li span.inner01,.person_infor li span.inner01{ background:url(https://passport.lefeng.com/images/login_ico.png) no-repeat left -545px;}
.sltBind li span.inner01 a,.authenCon li span.inner01 a,.person_infor li span.inner01 a{ color:#e63b00;}
.sltBind li span.inner02,.authenCon li span.inner02,.person_infor li span.inner02{ background:url(https://passport.lefeng.com/images/login_ico.png) no-repeat left -470px; color:#e63b00; }
.sltBind li span.inner03,.authenCon li span.inner03,.successPayMsg span.inner03,.person_infor span.inner03{ background:url(https://passport.lefeng.com/images/login_ico.png) no-repeat left -505px; }
.veriImg img{ width:68px; height:24px; border:1px solid #000; cursor:pointer;}
.veriH{ height:26px; line-height:26px;}
.veriH i{ font-style:normal; color:#989898;}
.veriH a{ color:#0086cf;}
.sltBtnArea{ padding-left:90px;}
.sltBtnArea a{ width:79px; height:32px;display:inline; float:left; text-align:center; line-height:32px; margin-right:10px;}
.sltBtnArea a.bindBtn{ background-position:0px -256px;}
.sltBtnArea a.bindBtn:hover{ background-position:0px -289px;}
.sltBtnArea a.sltBtn{ background-position:-79px -256px;}
.sltBtnArea a.sltBtn:hover{ background-position:-79px -289px;}
.sltTips{ background:#fffcd6; overflow:hidden; border:1px solid #fdb381; color:#3c3c3c; padding:0px 20px 10px; overflow:hidden; zoom:1; margin:10px 0px 0px;}
.sltTipsH{ font-weight:bold; height:36px; line-height:36px; color:#535353;}
.sltTips p{ line-height:22px;}
.bindFloat{ width:328px; border:1px solid #ddd; padding:0px 0px 20px 0px; color:#535353;}
.bindFloatH{ background-position:0px -326px; background-repeat:repeat-x; font-weight:bold; height:34px; line-height:34px; color:#535353;}
.bindFloatH a{ width:11px; height:11px; display:inline-block; float:right; background-position:-165px -294px; margin:12px 12px 0px 0px;}
.bindFloatH a:hover{ background-position:-181px -294px;}
.bindFloat ul{ margin:10px 10px 0px; overflow:hidden; zoom:1;}
.bindFloat li{ line-height:20px; clear:both; width:308px; display:inline; float:left;}
.bindFloat li em{ width:105px; display:inline; float:left; text-align:right;}
.bindFloat li span{ display:inline; float:left;}
.bindFloat li.bindFloatBtn{ height:32px; line-height:32px; margin-top:4px;padding-left: 104px;}
.bindFloat .bFconfirmBtn{width:79px; height:32px;display:inline; float:left; text-align:center; line-height:32px;background-position:0px -256px;margin-right: 5px;}
.hasBind,.myCartList{ padding:0px 0px 20px 0px; overflow:hidden; zoom:1;}
.hasBind table,.myCartList table{ width:738px; margin:0px 15px;}
.hasBind tr th,.myCartList tr th{ border-bottom:1px solid #ddd; height:46px; line-height:46px; font-weight:bold; text-align:center;}
.hasBind tr td,.myCartList tr td{ border-bottom:1px dotted #ddd; height:40px; line-height:40px; text-align:center;}
.hasBind .cartKinds{ width:70px; text-align:center;}
.hasBind .cartNum{ width:198px; text-align:center;}
.hasBind .cartFace{ width:76px; text-align:center;}
.hasBind .cartBalance{ width:105px; text-align:center;}
.hasBind .cartValid{ width:189px; text-align:center;}
.hasBind .csmDetails{ width:98px; text-align:center;}
.hasBind .seeBtn,.myCartList .openCartBtn,.myCartList .bindingBtn{ background-position:-163px -257px; width:44px; height:28px; line-height:28px; text-align:center; display:inline-block; margin:0 auto;}
.hasBind .seeBtn:hover,.myCartList .openCartBtn:hover,.myCartList .bindingBtn:hover{ background-position:-209px -257px;}

/*分页*/
.page3{ text-align:right; zoom:1; padding:12px 40px 0px;}
.page3 input.text{ text-align:center; padding-left:0;}
.page3 p.text_l{ line-height:26px;}
.page3 a,.page3 span{ display:inline-block; border:1px solid #ccc; padding:1px 6px; color:#000; margin:0 4px; vertical-align:middle; font-weight:normal;}
.page3 a:hover{ border:1px solid #963c00;}
.page3 span.current{ background-color:#963C00; color:#fff; border:1px solid #963C00;}
.page3 a.end{ color:#CBCBCB;}
.page3 a.end:hover{ text-decoration:none;}
.myCartList .cartKinds{ width:70px;}
.myCartList .orderNum{ width:100px;}
.myCartList .cartNum{ width:118px;}
.myCartList .cartFace{ width:88px;}
.myCartList .cartStatus{ width:78px;}
.myCartList .cartValid{ width:161px;}
.myCartList .cartOp{ width:160px;}
.sendPwdToMobile{ background-position:0px -398px; width:94px; height:28px;margin-left:5px; line-height:28px; display:inline-block;}
.sendPwdToMobile:hover{background-position:-98px -398px; margin-left:5px;}
/*  paypwd   */
.paypwd{ border:1px solid #ddd; overflow:hidden; zoom:1;}
.paypwdHead{background-position: 0 -222px;background-repeat: repeat-x; height:34px; text-indent:15px; font-weight:bold; line-height:34px;}
.paypwdFlow{ width:740px; margin:0px 10px; padding:10px 0px; border-bottom:1px solid #ddd; overflow:hidden;}
.paypwdFlow span{ display:inline; float:right; width:466px; height:35px; line-height:35px; position:relative; overflow:hidden; zoom:1;}
.paypwdFlow span em{ position:absolute; top:0px; height:35px; line-height:35px; color:#963c00;}
.paypwdFlow span.step1{ background:url(http://img3.imglefeng.com/images/cart/flowStep1.jpg) no-repeat;}
.paypwdFlow span.step2{ background:url(http://img3.imglefeng.com/images/cart/flowStep2.jpg) no-repeat;}
.paypwdFlow span.step3{ background:url(http://img3.imglefeng.com/images/cart/flowStep3.jpg) no-repeat;}
.paypwdFlow span .tep1{ left:65px;}
.paypwdFlow span .tep2{ left:196px;}
.paypwdFlow span .tep3{ left:385px;}
.paypwdFlow span em.on{ color:#fff;}
.authenCon{ margin:0px 10px; padding:0px 0px 20px; overflow:hidden;}
.authenCon  li{ margin-top:15px; display:inline-block;  width:745px; clear:both;}
.authenCon  li em{ display:inline; float:left; width:115px; padding:0px 10px; text-align:right; color:#535353; height:26px; line-height:26px;}
.authenCon  li input{ width:180px; height:24px; line-height:24px; border:1px  solid #ddd; display:inline; float:left;}
.authenLi{ line-height:28px;}
.authenLi i,.authenLi input,.authenLi b{ display:inline; float:left;}
.authenLi i{ font-style:normal; color:#666;}
.authenCon  li .authenLi input{ width:104px; height:28px; background-position:0px -362px; border:0px; text-align:center; line-height:28px; margin-left:15px; cursor:pointer;}
.authenLi b{ color:#963c00; font-weight:bold; margin-left:10px;}
.checkTips{ width:415px; padding:7px 10px; overflow:hidden; zoom:1; color:#3c3c3c; line-height:20px; background:#fffcd6; border:1px solid #fdb381; clear:both; margin:10px 0px 0px 135px; display:inline; float:left;}
.checkTips b{ font-weight:bold;}
.checkTips i{ font-style:normal; color:#963c00;}
.authenBtn .submitBtn{ background-position: 0 -256px; border:0px; height: 32px;line-height: 32px; margin-right: 10px; text-align: center;width: 79px; display:inline; float:left; margin-left:135px;}
.authenBtn .submitBtn:hover{background-position: 0 -289px;}
.successPayMsg{ text-align:center; padding:40px 0px;}
.successPayMsg span{ padding:6px 20px 0px;}

/*  用卡  */
.oneSub_tip{ margin-top:10px;}
.red{ color:#FF0104;}.zong{ color:#953B00;}.font_w{font-weight:bold;}
.use_lpk{ width:1000px; margin:0 auto; overflow:hidden; clear:both; border-top:2px solid #903700; background-color:#F9F9F9;}
.use_lpk h3{ height:37px; line-height:37px;line-height:41px\9; overflow:hidden; border-bottom:1px solid #ddd;}
.use_lpk h3 span{ display:inline-block; background:url(http://img3.imglefeng.com/images/cart/lpk_icon.gif) 15px 13px no-repeat; padding-left:32px; font-weight:bold; color:#333;}
.use_lpk h3 a:hover{ background:url(http://img3.imglefeng.com/images/cart/lpk_icon.gif) 15px -13px no-repeat; color:#903700;}
.use_lpk h3 span.on{ background:url(http://img3.imglefeng.com/images/cart/lpk_icon.gif) 15px -39px no-repeat; color:#903700;}

.person_infor{ padding:20px 0 5px 0; overflow:hidden; zoom:1;}
.person_infor h2{ font-size:14px; color:#333; padding:10px 0 20px 57px;}
.person_infor li{ line-height:27px; padding-bottom:15px; position:relative;}
.person_infor li em	{ display:inline-block; width:160px; text-align:right; color:#535353;}
.person_infor li em.red{ color:#FF0104;}
.person_infor li i{ font-style:normal;}
.person_infor li i b{ padding-right:15px;}
.person_infor .text	{ width:268px; height:32px; line-height:32px; border:1px solid #ccc; color:#999; vertical-align:middle;border-radius:2px; padding-left:5px; box-shadow:1px 1px 1px #eee;}
.person_infor .text.focus{ border:1px solid #E2901A;}
.person_infor .text.cuo{ border:1px solid #FF0000;}

.person_infor li span{ display:non; text-align:left; position:absolute; left:450px; top:3px; height:30px;}
.lpk_bt{ overflow:hidden; padding-left:163px;}
.lpk_btn{ width:91px; height:34px; line-height:34px; display:inline-block; background:url(http://img3.imglefeng.com/images/cart/lpk_bt.jpg) 0 -1px no-repeat; text-align:center; color:#333; margin-right:8px;}
.lpk_btn:hover	{ background-position:0 -40px; text-decoration:none; color:#333;}
.lpk_btn.on2{ background-position:0 -79px; color:#999;}
.lpk_btn2{ width:91px; height:34px; line-height:34px; display:inline-block; background:url(http://img3.imglefeng.com/images/cart/lpk_bt.jpg) -100px -1px no-repeat; text-align:center; color:#333;}
.lpk_btn2:hover	{ background-position:-100px -40px; text-decoration:none; color:#333;}
.lpk_btn2.on2{ background-position:-100px -79px; color:#999;}

.order_suc{ width:890px; margin:20px auto 0; padding:25px 0 25px 70px; overflow:hidden; clear:both; background:url(http://img3.imglefeng.com/images/cart/gou.gif) 20px 28px no-repeat #FFF6D9;}
.order_suc h2{ color:#000; font-size:18px; font-family:Microsoft YaHei; padding-bottom:10px;}
.order_suc p{ font-size:14px; padding-bottom:10px;}
.order_suc p b{ color:#983B00; padding-right:25px; font-weight:bold;}
.order_suc em{ color:#FF0104;}
/*common*/
#box_loading{height:32px;width:150px; text-align:right;line-height:32px; background:#FFF;display: none;
    overflow: hidden; position: absolute;font-size:20px; background:url(http://img3.imglefeng.com/images/cart/loading.gif) no-repeat #FFF; padding-right:10px; z-index:2015; font-family:microsoft yahei;}
.maskfuck{background-color: #000000;opacity:0.7;filter:alpha(opacity=60);}

/*     添加密码强度判断    */
.level{width:180px;height:20px;margin:5px 0 0; padding-left:135px; clear:both; overflow:hidden; zoom:1;}
.level b{float:left;width:59px;height:14px;line-height:14px;text-align:center;background:#c6b5ab;margin:0 1px 0 0;color:#fff;}
.level b.on{background:#ffaa01; font-weight:bold;}
/*   	替代默认提示框样式	   */
.payPwdMmsg{ border:1px solid #FDB381; background:#FFFCD6; width:666px; height:55px; margin:10px 0 15px;}
.payPwdMmsg p{ padding:8px; line-height:20px; color:#555;}
.payPwdMmsg p a{ color:#09F; text-decoration:underline;}

/*    	试用礼品卡提示			*/
.use_tip{ background:#ffffe5; border:1px solid #ffcc7f;overflow:hidden; zoom:1;}
.use_tip p{ padding:10px 40px; overflow:hidden; zoom:1;}