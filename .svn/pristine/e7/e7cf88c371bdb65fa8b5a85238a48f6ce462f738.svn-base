.hy-top-btn a{float:left;display:inline-block;color:#666;line-height:26px;height:26px;width:90px;text-align:center;position:relative;border:1px solid #c6a756;margin-right:15px;}
.hy-top-btn a.on{font-weight:bold;color:#fff; background:#c6a756;}
.hy-record{height:145px;text-align:center;}
.hy-record span.fontBold,.hy-number dt,.hy-RewardSp span,.hy-recommend span{font-weight:bold;font-size:14px;margin-bottom: 20px;display:block;margin-top:30px;}
.hy-RewardSp span.fTnormal{ font-weight:normal;}
