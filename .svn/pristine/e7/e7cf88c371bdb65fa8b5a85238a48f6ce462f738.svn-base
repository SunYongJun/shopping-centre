.hy-RewardSp li a img{width:130px;height:130px;display:inline-block;}
.hy-RewardSp li a.fT00aa18{color:#00aa18;}
.hy-RewardSp li a.fT999{color:#999;}
.hy-recommend,.hy-modality{width:738px;height:180px;border:1px solid #ece5d9;text-align:center; }
.hy-modality{width:694px;height:440px;padding:22px;}
.hy-RewardSp ul.bodtopNo{border-top:none;}
.hy-link{margin-bottom:30px;}
.hy-link dt,.hy-email .fsT{text-align:left;font-weight:bold;}
.hy-link dd{padding:10px 0px 0px 43px;text-align:left;}
.hy-link dd input.text{line-height:30px;width:470px;color:#999;}
.hy-link dd input.button,.sendmail input{width:90px;height:35px;line-height:35px;text-align:center;background: url(../images/fengchao_btn2.png);border:none;cursor:pointer;}
.hy-email dt{float:left;width:65px;line-height:30px;text-align:left;display:inline;margin-left:43px;margin-top:10px;}
.hy-email dd{float:left;width:585px;text-align:left;margin-top:10px;}
.hy-email dd input.put1,.hy-email dd input.put2{width:190px;line-height:30px;color:#666;padding-left:10px;}
.hy-email dd input.put2{width:390px;}
.hy-email dd textarea{width:585px;height:105px;margin-bottom:10px;color:#666;}
.sendmail{text-align:left;margin-left:107px;}
