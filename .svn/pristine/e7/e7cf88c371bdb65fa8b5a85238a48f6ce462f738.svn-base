.hy-RewardSp .hy-public-btn1,.hy-record .hy-public-btn1 {width:90px;background: url(../images/sign.png) left -202px no-repeat;color: #fff;line-height: 30px;text-decoration: none;text-align: center;cursor:pointer;}
.hy-tb td{padding:0px;}
.hy-number dt,.hy-RewardSp span{width:740px;}
.hy-number dt a,.hy-RewardSp span a{font-weight:normal;float:right;display: inline-block;color: #01B1CB;font: normal 12px simsun;}
