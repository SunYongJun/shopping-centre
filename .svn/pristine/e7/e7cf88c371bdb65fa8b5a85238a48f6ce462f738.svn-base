.pn-article .how_gift{width:350px;color:#000;font:12px '微软雅黑';position:absolute;left:0px;top:55px;}
.pn-article .how_gift dl dd{color:#000;}
.pn-article .how_gift dt{font-size:18px;margin-bottom:15px; font-weight:100;width:100px;text-align:left;color:#000;}
.how_gift dt,.how_gift dd{line-height:30px;margin-left:25px;}
.how_gift .lf-fr{width:13px;height:13px;position:absolute;right:0px;background:url(../images/btn7.png) left -18px no-repeat;display:inline-block!important;cursor:pointer}
.sysm{position:relative;}
.hf{ width:680px;}
