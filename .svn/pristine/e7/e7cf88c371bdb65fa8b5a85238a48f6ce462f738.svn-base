.hy-number dd span.bordNo,.hy-RewardSp ul li.bordNo{border:none;}
.hy-number dd span cite,.hy-RewardSp li a cite{color:#f52648;padding:0px 5px;font-weight:bold;}
.hy-RewardSp li a cite{padding:0px;}
.hy-number dd span a,.hy-RewardSp a.hy-public-btn2{width:90px;background: url(../images/sign.png) left -235px no-repeat;line-height: 30px;text-decoration: none;text-align:center;}
