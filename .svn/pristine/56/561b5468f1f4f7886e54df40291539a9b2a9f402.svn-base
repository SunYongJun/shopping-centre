@charset "utf-8";
/* CSS Document */
html{background-color:#F1EBDD!important;background-image:url(about:blank)!important;background-attachment:fixed!important; overflow-x:hidden;}
.sub-nav p b,.srhMulBrandsArea,.nav .selList a b,.nav ul li .more a i, .nav ul li .hide a i,.more_mod dl dt i,.order b,.tpages .prev,.tpages .next,.makeup dl .pri .to,.brandPicArea em b,.makeup dl dt i{background:url(../images/sico.gif) no-repeat;}
.ClearFix:after{content:'.';display:block;height:0;clear:both;overflow:hidden;visibility:hidden;}
.ClearFix{*zoom:1;}
.mainBg{ width:100%;  /*zoom:1;*/ background:#f1ebdd;}
