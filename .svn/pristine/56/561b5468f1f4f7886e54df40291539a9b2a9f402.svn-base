.sub-nav li a:hover,.sub-nav li a.down{color:#e8254a;}
.skimbuy,.pankhis{font-family:sumsun,arial; box-shadow:2px 2px 5px #e6e2e2; background:#faf8f8; margin-top:10px;}
.skimbuy{ padding-bottom:30px; overflow:hidden; zoom:1;}
.skimbuy h3,.pankhis h3{font:normal 14px/54px Microsoft YaHei; color:#333;text-indent:20px;border-bottom:1px solid #eceae6;}
