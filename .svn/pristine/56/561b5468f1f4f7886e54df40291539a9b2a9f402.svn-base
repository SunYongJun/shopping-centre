.nav ul li .addsel,.nav ul li .addselBtn{position:absolute;top:10px;right:50px;color: #CA8516;cursor: pointer;width: 36px; height: 22px;line-height: 22px;overflow: hidden;z-index: 22;}
.nav ul li .addselBtn{display:none;}
.srhMulBrands{ overflow:hidden; zoom:1; display: inline; float: right;width: 825px;margin-top:5px;}
