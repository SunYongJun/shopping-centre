.nav .selList a{position:relative;background:#f5f1f1; float:left;height:24px;padding:0 20px 0 0px; line-height:24px;white-space:nowrap;/*min-width:40px;_width:40px;*/cursor:pointer;}
.nav .selList a:hover{ text-decoration:none;}
.nav .selList a em{ height:24px; padding:0px 0px 0px 10px; line-height:24px; /*min-width:40px; _width:40px;*/ display:inline-block;white-space:nowrap;}
.nav .selList a b{position:absolute;top:4px;right:4px;background-position:-100px 0; width:15px; height:15px;overflow:hidden;}
.nav .selList a:hover b{background-position:-120px 0;}
.nav .sel .cancelCheck{ display:inline; float:right;}
.nav .cancelCheck a{ text-decoration:underline; color:#999;}
.nav ul{ clear:both;}
.nav ul li{ border-bottom:1px solid #eceae6; padding:7px 0px; overflow:hidden; zoom:1;position:relative; width:920px;height:28px;overflow:hidden;}
.nav ul li b,.more_nav b{float:left;width:70px;text-align:justify; text-justify:inter-word;/*ie6-8*/ text-align-last:justify;/* ie9*/-moz-text-align-last:justify;/*ff*/-webkit-text-align-last:justify;/*chrome 20+*/color:#999;overflow:hidden;white-space:nowrap;font-weight:bold; line-height:28px;}
.nav ul li .mod{float:left;width:730px; margin-left:23px;}
.nav ul li .mod span{position:relative;float:left;width:160px; margin:0px 15px 0px 5px; height:28px;overflow:hidden; line-height:28px;}	
.nav ul li .mod a{ color:#333;white-space:nowrap;float:left;max-width:110px;overflow:hidden;}  
.nav ul li .mod span img{position:relative;left:2px;top:1px;_display:none;}
.nav ul li .mod span a i{ font-style:normal; font-family:Arial; color:#999;} 
.nav ul li .more,.nav ul li .hide{position:absolute;top:10px;right:0px;width:36px;height:22px; line-height:22px;color:#ca8516;cursor:pointer;overflow:hidden;z-index:22;}
.nav ul li .more a,.nav ul li .hide a{ color:#ca8516;}
.nav ul li .more a i,.nav ul li .hide a i{ background-position:-140px 0; width:7px; height:4px; float:right; display:inline-block; margin:8px 0px 0px;}
.nav ul li .hide a i{background-position:-150px 0;}
.nav ul li .more a:hover i{background-position:-10px 0;}
.nav ul li .hide a:hover i{background-position:-160px 0;}
.nav ul li .more a:hover,.nav ul li .hide a:hover{ color:#e8254a;}
.nav ul li .mod a:hover{ color:#e8254a;}
.main .mod{width:980px;}
.main .mod span{margin: 0px 15px 0px 5px; }
.list960 .mod{width:580px!important;}
.list960 .mod span a{max-width:90px;}
.list960 .mod span{margin: 0px 0px 0px 5px!important;width:120px!important;}
