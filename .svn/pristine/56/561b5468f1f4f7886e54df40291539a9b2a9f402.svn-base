.nav ul li em span{float:left;width:130px; display:inline;height:22px; line-height:22px; overflow:hidden;}
.nav ul li em span a{ margin:0px 0x 0px 10px; max-width:98px;overflow:hidden;}
.nav ul li em a,.nav ul li em  em{float:left;position:relative;white-space:nowrap;}
.nav ul li em  span input,.nav ul li .mod span input{ float:left; margin:3px 10px 0px 0px; width:15px; height:15px;background:#eceae6;}
.nav ul li em  a{color:#333;}
.nav ul li em  a:hover,.nav ul li .mod a:hover{ color:#e8254a;}
.nav ul li em  a i{font-style:normal;font-weight:normal; color:#999;}
.nav ul li em  em{color:#999;cursor:default;}
.nav ul li em  .down i,.nav ul li em  .d i{ background:url(http://img1.imglefeng.com/images/list/searchIcon.png) no-repeat; position:absolute;left:45%;bottom:-3px;width:5px;height:3px;background-position:0px -45px;font-size:0;}




