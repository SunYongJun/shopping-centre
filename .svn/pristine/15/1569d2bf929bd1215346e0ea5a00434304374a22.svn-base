/*自定义的bootstrap的ss样式，用来辅助排版*/

/*页面分页*/
.fenye a{
	font-family:'Open Sans';
	font-size:13px;
	color:#6FAED9;
	text-decoration: none;
	padding:6px 12px;
	border:1px solid #E0E8EB;
}
.fenye a:hover{
	color:#fff;
	background: #6FAED9;
	border:1px solid #6FAED9;
}
.fenye .current{
	font-family:'Open Sans';
	font-size:13px;
	color:#fff;
	background: #6FAED9;
	padding:6px 12px;
	border:1px solid #6FAED9;
}

/*下拉菜单*/


.drop_down{
	border:1px solid #ccc;
	width:250px;
}
.drop_down+ul{
	width:250px;
}
.dropdown_sp,.dropdown_sp>a{
	font-size:13px;
	color:#444;
}

.dropdown_sp>a:hover{
	text-decoration:none;
}

//商品相册


	#mask{
		height:1003px;
		width:100%;
		background:#000;
		position:absolute;
		top:0px;
		left:0px;
		z-index:1000;
		opacity:0.75;
		filter:alpha(opacity=0.75);
	}


	.chk{height: 30px;}